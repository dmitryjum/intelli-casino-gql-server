    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^6.0.0",
